	<!--Knif do animacji-->
	<include name="KnifDoAnimacji">
		<control type="image" id="9923078">
            <include>InvisibleButton</include>
        	<animation effect="fade" start="0" end="100" time="150" delay="1000">Visible</animation>
            <visible>![Container(6010).OnNext | Container(6010).OnPrevious] + Control.HasFocus(6010)</visible>
        </control>
	</include>

	<!--Animacja CD na liscie-->
	<include name="AnimacjaCD">
		<animation type="conditional" reversible="false" condition="true">
				<effect type="slide"  delay="2000" end="120,0" time="1000" easing="out"/>
				<effect type="rotate" delay="2000" start="0"   end="-57"   time="1000" center="auto"  loop="true" easing="out"/>

				<effect type="rotate" delay="3000" end="-303"     time="4000" center="auto"  loop="true" easing="out"/>

				<effect type="slide"  delay="12000" end="-120,0" time="1000" easing="out"/>
				<effect type="rotate" delay="12000" end="57"     time="1000" center="auto"  loop="true" easing="out"/>

				<effect type="rotate" delay="13000" end="303"     time="1000" center="auto"  loop="true" easing="out"/>
        </animation>
	</include>

	<!--Animacja CD muzyka -->
	<include name="AnimacjaCD_Play">
		<animation effect="rotate" start="0" end="-360" center="auto" time="40000" delay="6000" loop="true" reversible="false" condition="!Player.Playing">Conditional</animation>
		<animation effect="rotate" start="0" end="-360" center="auto" time="4000" loop="true" reversible="false" condition="Player.Playing">Conditional</animation>
	</include>

	<include name="EfektCD">
		<effect type="slide"  delay="2000" end="320,0" time="1000" easing="out"/>
		<effect type="rotate" delay="2000" start="0"   end="-57"   time="1000" center="auto"  loop="true" easing="out"/>

		<effect type="rotate" delay="3000" end="-303"     time="4000" center="auto"  loop="true" easing="out"/>

		<effect type="slide"  delay="12000" end="-320,0" time="1000" easing="out"/>
		<effect type="rotate" delay="12000" end="57"     time="1000" center="auto"  loop="true" easing="out"/>

		<effect type="rotate" delay="13000" end="303"     time="1000" center="auto"  loop="true" easing="out"/>

		<effect type="rotate" delay="15000" end="-360"     time="40000" center="auto"  loop="true" easing="out"/>
	</include>

</includes>